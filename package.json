  "dependencies": {
    "@erc725/erc725.js": "^0.25.0",
    "@lukso/lsp-smart-contracts": "^0.15.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "ethers": "^6.11.0",
    "inquirer": "^9.2.0",
    "ora": "^8.0.0",
    "qrcode": "^1.5.3",
    "table": "^6.8.1",
    "viem": "^2.45.1"
